# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/appdb

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
S3_BUCKET_NAME=your-bucket-name

# LocalStack (for local development)
AWS_ENDPOINT_URL=http://localhost:4566

# Textract SNS/SQS (LocalStack)
TEXTRACT_SNS_TOPIC_ARN=arn:aws:sns:us-east-1:000000000000:textract-results
TEXTRACT_ROLE_ARN=arn:aws:iam::000000000000:role/TextractServiceRole
TEXTRACT_SQS_QUEUE_URL=http://localhost:4566/000000000000/textract-queue

# Server
PORT=3000
NODE_ENV=development